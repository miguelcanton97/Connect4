<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/chip_red" />
            <size
                android:width="17dp"
                android:height="17dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <size
                android:width="8dp"
                android:height="8dp" />
            <stroke
                android:width="1.5dp"
                android:color="@color/white50"
                android:dashGap="2dp"
                android:dashWidth="2dp"
                />
        </shape>
    </item>
</layer-list>