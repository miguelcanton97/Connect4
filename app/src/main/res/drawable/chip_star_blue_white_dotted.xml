<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/chip_blue" />
            <size
                android:width="17dp"
                android:height="17dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <size
                android:width="8dp"
                android:height="8dp" />
            <stroke
                android:width="1.5dp"
                android:color="@color/white"/>
        </shape>
    </item>

    <item
        android:width="13dp"
        android:height="13dp"
        android:gravity="center"
        android:drawable="@drawable/star_white"/>
</layer-list>


